  <cheat desc="Hide Background"> <comment>Activate before match start</comment>
    <script state="on">
      <action>temp0=maincpu.rw@00C21E</action>
      <action>temp1=maincpu.rd@00C220</action>
    </script>
    <script state="run">
      <action condition="(maincpu.pw@1093CC == 0300)">maincpu.rw@00C21E=4E71</action><!-- activate only during match --><!-- HUD -->
      <action condition="(maincpu.pw@1093CC == 0300)">maincpu.rd@00C220=4E714E71</action><!-- HUD -->
      <action condition="(maincpu.pw@1093CC == 0300)">maincpu.pw@1093C4=000F</action><!-- BG layers -->
      <action condition="(maincpu.pw@1093CC == 0300)">maincpu.pw@1093C6=8F8F</action><!-- BG colour (pink) -->
    </script>
    <script state="off">
      <action>maincpu.rw@00C21E=temp0</action>
      <action>maincpu.rd@00C220=temp1</action>
      <action>maincpu.pw@1093C4=0000</action>
      <action>maincpu.pw@1093C6=7DFF</action>
    </script>
  </cheat>

